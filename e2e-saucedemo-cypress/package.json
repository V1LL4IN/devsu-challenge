{
  "name": "e2e-saucedemo-cypress",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress run --headed",
    "open": "cypress open",
    "report:merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json",
    "report:html": "marge cypress/reports/merged.json -o cypress/reports/html --inline",
    "posttest": "npm run report:merge && npm run report:html"
  },
  "devDependencies": {
    "cypress": "^13.7.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.3.1"
  }
}